url":"","only_b2c":false,"family_enabled":true,"key":"travel_insurance"},{"attributes":[],"toggle":false,"amount":0,"only_b2b":false,"currency":"INR","chargable":false,"source_url":"","only_b2c":false,"family_enabled":false,"key":"travel_itinerary"},{"attributes":[],"toggle":false,"amount":0,"only_b2b":false,"currency":"INR","chargable":false,"source_url":"","only_b2c":false,"family_enabled":false,"key":"us_bank_statements"},{"attributes":[],"toggle":false,"amount":0,"only_b2b":false,"currency":"INR","chargable":false,"source_url":"","only_b2c":false,"family_enabled":true,"key":"us_residence"},{"attributes":[],"toggle":false,"amount":0,"only_b2b":false,"currency":"INR","chargable":false,"source_url":"","only_b2c":false,"family_enabled":true,"key":"us_uk_schengen_visa"}],"additional_questions":[{"label":"As directed by the Govenment of Kenya, Please upload round trip flight ticket is mandatory.","required":true,"only_b2b":false,"extra_info":"","required_doc":"","question_type":"file","source_url":"","description":"As directed by the Govenment of Kenya, Please upload round trip flight ticket is mandatory.","family_enabled":true,"key":"RoundtripFlightTicket"},{"label":"Hotel Booking Details","required":true,"only_b2b":true,"extra_info":"","required_doc":"Hotel Booking Details","question_type":"file","source_url":"","description":"As directed by the Govenment of Kenya, Hotel Booking Details Is Mandatory.","family_enabled":true,"key":"HotelBooking"}],"timestamp":"2025-01-08T11:03:59.932Z","dynamic_eta_fusion":[{"unit":"days","duration":60,"on_event":"pre_travel"},{"unit":"days","duration":0,"on_event":"post_appointment"},{"unit":"days","duration":4,"on_event":"post_checkout"}],"last_edited_by":"Programatically"},"partners":{"id":"rxsmrp0un59wjhb75b40exqk","type":"clp_partners","items":[{"logo":{"alt":"IATA logo.","url":"https://images.prismic.io/atlys/d5504196-40e8-41ba-a58d-768ccab8807d_iata.png?auto=compress,format&rect=0,0,356,240&w=356&h=240"}},{"logo":{"alt":"Republic of Kenya logo.","url":"https://images.prismic.io/atlys/8a961c6d-80ef-4f3a-848b-7962ab1ee81b_Kenya+Government.png?auto=compress,format"}},{"logo":{"alt":"Kenya e-citizen logo.","url":"https://images.prismic.io/atlys/6d5a5434-b0a2-4a6b-bb4f-00c813a67f45_Kenya+e-citizen+logo.png?auto=compress,format"}}]},"currency":"AED","faqs":{"id":"exb62qxhskk66u18zrwevzdx","type":"blog_faq","title":"Frequently Asked Questions","items":[{"question":"Do Indians need a visa to visit Kenya?","answer":{"type":"doc","content":[{"type":"paragraph","attrs":{"textAlign":"left"},"content":[{"text":"Yes. ","type":"text","marks":[{"type":"textStyle"},{"type":"bold"}]},{"text":"Indian citizens visiting Kenya need a mandatory Electronic Travel Authorisation (eTA). An eTA offers permission to travel and is authorized by the Government of the Republic of Kenya.","type":"text","marks":[{"type":"textStyle"}]}]}]},"category":"General Information"},{"question":"What are the requirements for Indians to obtain a Kenya eTA?","answer":{"type":"doc","content":[{"type":"paragraph","attrs":{"textAlign":"justify"},"content":[{"text":"To complete the online application form for a Kenya eTA for Indians, you will need certain documents.","type":"text","marks":[{"type":"textStyle"}]}]},{"type":"bulletList","content":[{"type":"listItem","content":[{"type":"paragraph","attrs":{"textAlign":"justify"},"content":[{"text":"Passport copy:","type":"text","marks":[{"type":"textStyle"},{"type":"bold"}]},{"text":" A clear, colour scan of the information page (typically page 2 or 3) and the last page of your passport. These pages contain your personal details, passport number, and visa stamps (if applicable).","type":"text","marks":[{"type":"textStyle"}]}]}]},{"type":"listItem","content":[{"type":"paragraph","attrs":{"textAlign":"justify"},"content":[{"text":"Passport-sized photograph:","type":"text","marks":[{"type":"textStyle"},{"type":"bold"}]},{"text":" A recent, high-quality colour photograph taken against a plain white background. Ensure the ","type":"text","marks":[{"type":"textStyle"}]},{"text":"photo adheres to the specific dimensions","type":"text","marks":[{"type":"link","attrs":{"id":null,"rel":null,"url":"https://www.atlys.com/tools/kenya-visa-photo-maker","slug":null,"type":null,"class":"link","locale":null,"target":null,"link_type":"web"}},{"type":"textStyle"}]},{"text":" and guidelines provided by the relevant authorities.","type":"text","marks":[{"type":"textStyle"}]}]}]}]}]},"category":"Eligibility & Requirements"},{"question":"How do I apply for a Kenya eTA from India? ","answer":{"type":"doc","content":[{"type":"paragraph","attrs":{"textAlign":"justify"},"content":[{"text":"You can visit the official government channel if you opt to apply for an online Kenya eTA.  ","type":"text","marks":[{"type":"textStyle"}]}]},{"type":"bulletList","content":[{"type":"listItem","content":[{"type":"paragraph","attrs":{"textAlign":"left"},"content":[{"text":"Visit the official channel:","type":"text","marks":[{"type":"textStyle"},{"type":"bold"}]},{"text":" Visit the Kenyan eTA website.","type":"text","marks":[{"type":"textStyle"}]}]}]},{"type":"listItem","content":[{"type":"paragraph","attrs":{"textAlign":"left"},"content":[{"text":"Start your application:","type":"text","marks":[{"type":"textStyle"},{"type":"bold"}]},{"text":" Click on the \"Apply Now\" button and follow the on-screen instructions.","type":"text","marks":[{"type":"textStyle"}]}]}]},{"type":"listItem","content":[{"type":"paragraph","attrs":{"textAlign":"left"},"content":[{"text":"Fill out the application form:","type":"text","marks":[{"type":"textStyle"},{"type":"bold"}]},{"text":" Please enter all the required information carefully and accurately. This includes your personal details, passport information, travel plans, and any other details requested.","type":"text","marks":[{"type":"textStyle"}]}]}]},{"type":"listItem","content":[{"type":"paragraph","attrs":{"textAlign":"left"},"content":[{"text":"Double-check your information:","type":"text","marks":[{"type":"textStyle"},{"type":"bold"}]},{"text":" Ensure all data is entered correctly before proceeding. Typos or errors might delay processing.","type":"text","marks":[{"type":"textStyle"}]}]}]},{"type":"listItem","content":[{"type":"paragraph","attrs":{"textAlign":"left"},"content":[{"text":"Upload documents:","type":"text","marks":[{"type":"textStyle"},{"type":"bold"}]},{"text":" Attach the necessary documents as specified by the website. This could include a scanned copy of your passport, a passport-sized photograph, and other supporting documents (if applicable).","type":"text","marks":[{"type":"textStyle"}]}]}]},{"type":"listItem","content":[{"type":"paragraph","attrs":{"textAlign":"left"},"content":[{"text":"Submit and pay:","type":"text","marks":[{"type":"textStyle"},{"type":"bold"}]},{"text":" Once everything is complete, submit your application and pay the processing fee using the available payment options.","type":"text","marks":[{"type":"textStyle"}]}]}]},{"type":"listItem","content":[{"type":"paragraph","attrs":{"textAlign":"left"},"content":[{"text":"Confirmation emails:","type":"text","marks":[{"type":"textStyle"},{"type":"bold"}]},{"text":" You'll typically receive two emails: one confirming receipt of your application and another (if applicable) confirming payment.","type":"text","marks":[{"type":"textStyle"}]}]}]},{"type":"listItem","content":[{"type":"paragraph","attrs":{"textAlign":"left"},"content":[{"text":"Decision notification:","type":"text","marks":[{"type":"textStyle"},{"type":"bold"}]},{"text":" The Kenyan immigration authorities will review your application. Once processed, you'll receive another email notifying you of either approval or rejection. Print the attached PDF document from the approval email.","type":"text","marks":[{"type":"textStyle"}]}]}]},{"type":"listItem","content":[{"type":"paragraph","attrs":{"textAlign":"left"},"content":[{"text":"Keep your eTA handy:","type":"text","marks":[{"type":"textStyle"},{"type":"bold"}]},{"text":" Your eTA is your electronic travel authorisation. Make sure to have it readily available for presentation at both departure and arrival points in Kenya.","type":"text","marks":[{"type":"textStyle"}]}]}]}]}]},"category":"Application Process"},{"question":"How can I check the status of my Kenya eTA?","answer":{"type":"doc","content":[{"type":"paragraph","attrs":{"textAlign":"justify"},"content":[{"text":"After applying for your visa, you can track its status online using the ","type":"text"},{"text":"Atlys Kenya visa status check tool","type":"text","marks":[{"type":"link","attrs":{"id":null,"rel":null,"url":"https://www.atlys.com/tools/kenya-visa-status-checker","slug":null,"type":null,"class":"link","locale":null,"target":null,"link_type":"web"}}]},{"text":".","type":"text"}]}]},"category":"Status Tracking"},{"question":"Do I need to submit my passport for the Kenya eTA process?","answer":{"type":"doc","content":[{"type":"paragraph","attrs":{"textAlign":"left"},"content":[{"text":"No,","type":"text","marks":[{"type":"textStyle"},{"type":"bold"}]},{"text":" Indians can provide a digital copy of their passport when completing the online eTA application. You will not need to submit your physical passport, but you must have it with you when you travel to Kenya, as you will present the eTA along with your passport upon arrival.","type":"text","marks":[{"type":"textStyle"}]}]}]},"category":"Application Process"},{"question":"How can I get my approved Kenya eTA? ","answer":{"type":"doc","content":[{"type":"paragraph","attrs":{"textAlign":"justify"},"content":[{"text":"After completing the Kenya eTA application online, you will receive an email notification of approval progress. After approval, you will get your approved eTA online, which can take a few days. Therefore, you’re advised to apply for the eTA as soon as you know your travel plans so you have enough time to process them.","type":"text"}]}]},"category":"Application Process"},{"question":"What does the Kenya eTA look like?","answer":{"type":"doc","content":[{"type":"imageBlock","attrs":{"src":"https://imagedelivery.net/W3Iz4WACAy2J0qT0cCT3xA/didi/articles/ydtrnfq0bkxkeovzwulpycng/public","align":"center","width":"100%"}}]},"category":"General Information"},{"question":"Do I need any vaccinations to enter the country?","answer":{"type":"doc","content":[{"type":"paragraph","attrs":{"textAlign":"left"},"content":[{"text":"No vaccinations are currently required to enter Kenya.","type":"text","marks":[{"type":"textStyle"}]}]}]},"category":"Entry & Exit Regulations"},{"question":"Do I need an eTA if I have a valid East Africa Tourist Visa?","answer":{"type":"doc","content":[{"type":"paragraph","attrs":{"textAlign":"left"},"content":[{"text":"No","type":"text","marks":[{"type":"textStyle"},{"type":"bold"}]},{"text":", a valid East Africa Tourist Visa allows you to travel to Kenya, Uganda, and Rwanda without the need for an additional eTA.","type":"text","marks":[{"type":"textStyle"}]}]}]},"category":"Entry & Exit Regulations"},{"question":"Can I change the travel dates on my existing eTA?","answer":{"type":"doc","content":[{"type":"paragraph","attrs":{"textAlign":"left"},"content":[{"text":"If you need to modify your travel dates after obtaining an eTA, you will generally need to contact the Kenyan immigration authorities directly. They can provide specific guidance on the process and requirements for updating your travel information.","type":"text","marks":[{"type":"textStyle"}]},{"type":"hardBreak"}]}]},"category":"Refunds, Rejections & Reapplications"},{"question":"Does an eTA guarantee entry into Kenya?","answer":{"type":"doc","content":[{"type":"paragraph","attrs":{"textAlign":"left"},"content":[{"text":"An eTA does not guarantee entry into Kenya. While it is a mandatory requirement for travel to Kenya, it is merely a pre-authorization. Upon arrival, immigration officials retain the right to question you and deny entry if they deem it necessary.","type":"text","marks":[{"type":"textStyle"}]}]},{"type":"paragraph","attrs":{"textAlign":"left"},"content":[{"text":"Therefore, having a valid eTA is essential but not sufficient for guaranteed entry.","type":"text","marks":[{"type":"textStyle"}]}]}]},"category":"Entry & Exit Regulations"},{"question":"Do foreign citizens married to Kenyan citizens require an eTA?","answer":{"type":"doc","content":[{"type":"paragraph","attrs":{"textAlign":"left"},"content":[{"text":"Yes,","type":"text","marks":[{"type":"textStyle"},{"type":"bold"}]},{"text":" all foreign visitors to Kenya, including spouses of Kenyan citizens, must obtain an eTA unless they hold a valid Kenyan Permanent Residence, Work Permit, or Pass.","type":"text","marks":[{"type":"textStyle"}]}]}]},"category":"Entry & Exit Regulations"},{"question":"Can I correct mistakes in my eTA application?","answer":{"type":"doc","content":[{"type":"paragraph","attrs":{"textAlign":"left"},"content":[{"text":"Yes, you can usually correct errors in your eTA application. Look for a link to update your submission in the confirmation email you received after applying. This link will allow you to access and modify your application details.","type":"text","marks":[{"type":"textStyle"}]}]},{"type":"paragraph","attrs":{"textAlign":"left"},"content":[{"text":"Note:","type":"text","marks":[{"type":"textStyle"},{"type":"bold"}]},{"text":" The availability of this option might depend on the specific eTA system.","type":"text","marks":[{"type":"textStyle"}]}]}]},"category":"Refunds, Rejections & Reapplications"},{"question":"Does the eTA allow multiple entries into Kenya?","answer":{"type":"doc","content":[{"type":"paragraph","attrs":{"textAlign":"left"},"content":[{"text":"Typically, a standard eTA is valid for a single entry into Kenya. However, there are exceptions:","type":"text","marks":[{"type":"textStyle"}]}]},{"type":"bulletList","content":[{"type":"listItem","content":[{"type":"paragraph","attrs":{"textAlign":"left"},"content":[{"text":"East African Community (EAC) travel:","type":"text","marks":[{"type":"textStyle"},{"type":"bold"}]},{"text":" If you visit other EAC countries (Burundi, Democratic Republic of Congo, Rwanda, South Sudan, Tanzania, or Uganda) without travelling to any other country, your single-entry eTA might allow re-entry into Kenya within its 90-day validity period.","type":"text","marks":[{"type":"textStyle"}]}]}]},{"type":"listItem","content":[{"type":"paragraph","attrs":{"textAlign":"left"},"content":[{"text":"Multiple entry eTA:","type":"text","marks":[{"type":"textStyle"},{"type":"bold"}]},{"text":" Citizens of the United States can apply for a five-year multiple entry eTA.","type":"text","marks":[{"type":"textStyle"}]}]}]}]},{"type":"paragraph","attrs":{"textAlign":"left"},"content":[{"text":"It's essential to check the specific conditions and requirements for your situation on the official Kenyan government website.","type":"text","marks":[{"type":"textStyle"}]}]}]},"category":"Entry & Exit Regulations"},{"question":"When should I apply for an eTA?","answer":{"type":"doc","content":[{"type":"paragraph","attrs":{"textAlign":"left"},"content":[{"text":"You can submit an eTA application up to 90 days before your planned travel date. While most applications are processed within three days, there can be exceptions. To ensure a smooth travel experience, it's recommended that you apply for your eTA at least two weeks before your departure date.","type":"text","marks":[{"type":"textStyle"}]}]}]},"category":"General Information"},{"question":"Can I apply for an eTA before booking my travel plans?","answer":{"type":"doc","content":[{"type":"paragraph","attrs":{"textAlign":"left"},"content":[{"text":"No,","type":"text","marks":[{"type":"bold"}]},{"text":" you cannot apply for an eTA before booking your travel arrangements. The application process requires specific details about your travel itinerary, including arrival and departure dates. Therefore, it's necessary to finalise your travel plans before starting the eTA application.","type":"text"}]}]},"category":"Eligibility & Requirements"},{"question":"How can I extend my stay in Kenya?","answer":{"type":"doc","content":[{"type":"paragraph","attrs":{"textAlign":"left"},"content":[{"text":"To extend your stay in Kenya, you must apply for a visitor's pass extension in person at the Immigration Services office located at Nyayo House, Nairobi. Be aware that there might be additional requirements and fees involved in this process. It's advisable to start the extension process well before your current permit expires to avoid any potential issues.","type":"text"}]}]},"category":"Visa Extension & Overstays"},{"question":"Why choose Atlys for your Kenya eTA application?","answer":{"type":"doc","content":[{"type":"paragraph","attrs":{"textAlign":"left"},"content":[{"text":"Atlys guarantees the timely delivery of visa and eTA as promised. Additional benefits of choosing Atlys as your visa partner include:","type":"text"}]},{"type":"bulletList","content":[{"type":"listItem","content":[{"type":"paragraph","attrs":{"textAlign":"left"},"content":[{"text":"Streamlined eTA application:","type":"text","marks":[{"type":"bold"}]},{"text":" Atlys simplifies the eTA application process, saving you time and effort.","type":"text"}]}]},{"type":"listItem","content":[{"type":"paragraph","attrs":{"textAlign":"left"},"content":[{"text":"User-friendly platform:","type":"text","marks":[{"type":"bold"}]},{"text":" The platform is designed for ease of use, making the application process straightforward and hassle-free.","type":"text"}]}]},{"type":"listItem","content":[{"type":"paragraph","attrs":{"textAlign":"left"},"content":[{"text":"Secure handling:","type":"text","marks":[{"type":"bold"}]},{"text":" Your personal information is handled with the utmost security and confidentiality.","type":"text"}]}]}]},{"type":"paragraph","attrs":{"textAlign":"left"}}]},"category":"General Information"},{"question":"How to apply for a Kenya eTA through Atlys?","answer":{"type":"doc","content":[{"type":"paragraph","attrs":{"textAlign":"left"},"content":[{"text":"Applicants easily complete the application on Atlys by following the below steps:","type":"text"}]},{"type":"bulletList","content":[{"type":"listItem","content":[{"type":"paragraph","attrs":{"textAlign":"left"},"content":[{"text":"Visit Atlys:","type":"text","marks":[{"type":"bold"}]},{"text":" You can start your application ","type":"text"},{"text":"here","type":"text","marks":[{"type":"link","attrs":{"id":null,"rel":null,"url":"https://www.atlys.com/en-IN/apply-kenya-visa","slug":null,"type":null,"class":"link","locale":null,"target":"_blank","link_type":"web"}}]},{"text":".","type":"text"}]}]},{"type":"listItem","content":[{"type":"paragraph","attrs":{"textAlign":"left"},"content":[{"text":"Click selfie: ","type":"text","marks":[{"type":"bold"}]},{"text":"You can click a selfie or upload your selfie using your phone camera. ","type":"text"}]}]},{"type":"listItem","content":[{"type":"paragraph","attrs":{"textAlign":"left"},"content":[{"text":"Upload documents: ","type":"text","marks":[{"type":"bold"}]},{"text":"Upload your passport’s front and last page.","type":"text"}]}]},{"type":"listItem","content":[{"type":"paragraph","attrs":{"textAlign":"left"},"content":[{"text":"Review: ","type":"text","marks":[{"type":"bold"}]},{"text":"You must review your application and make corrections. ","type":"text"}]}]},{"type":"listItem","content":[{"type":"paragraph","attrs":{"textAlign":"left"},"content":[{"text":"Pay the fee: ","type":"text","marks":[{"type":"bold"}]},{"text":"Pay the applicable eTA fee and checkout.","type":"text"}]}]},{"type":"listItem","content":[{"type":"paragraph","attrs":{"textAlign":"left"},"content":[{"text":"Approval: ","type":"text","marks":[{"type":"bold"}]},{"text":"After the approval, you will receive your eTA. ","type":"text"}]}]}]}]},"category":"Application Process"}]},"visas_done_in_k":22}}
