ent":[{"text":"Indian citizens can apply for the following types of Vietnam visas:","type":"text","marks":[{"type":"textStyle"}]}]},{"type":"bulletList","content":[{"type":"listItem","content":[{"type":"paragraph","attrs":{"textAlign":"justify"},"content":[{"text":"Tourist visa (DL): ","type":"text","marks":[{"type":"textStyle"},{"type":"bold"}]},{"text":"This visa is issued to foreigners visiting Vietnam for tourism purposes for generally up to 90 days. ","type":"text","marks":[{"type":"textStyle"}]}]}]},{"type":"listItem","content":[{"type":"paragraph","attrs":{"textAlign":"justify"},"content":[{"text":"Business visa (DN):","type":"text","marks":[{"type":"textStyle"},{"type":"bold"}]},{"text":" This visa is designed for those entering Vietnam for business purposes, including meetings, contracts, and partnerships.","type":"text","marks":[{"type":"textStyle"}]}]}]},{"type":"listItem","content":[{"type":"paragraph","attrs":{"textAlign":"justify"},"content":[{"text":"Student or internship visa (DH):","type":"text","marks":[{"type":"textStyle"},{"type":"bold"}]},{"text":" For individuals planning to study or participate in internship programs in Vietnam.","type":"text","marks":[{"type":"textStyle"}]}]}]},{"type":"listItem","content":[{"type":"paragraph","attrs":{"textAlign":"justify"},"content":[{"text":"Investor visa (DT):","type":"text","marks":[{"type":"textStyle"},{"type":"bold"}]},{"text":" Aimed at foreign investors and lawyers investing in Vietnam.","type":"text","marks":[{"type":"textStyle"}]}]}]},{"type":"listItem","content":[{"type":"paragraph","attrs":{"textAlign":"justify"},"content":[{"text":"Press visa (PV)","type":"text","marks":[{"type":"textStyle"},{"type":"bold"}]},{"text":": It is issued to resident foreign correspondents and reporters in Vietnam.","type":"text","marks":[{"type":"textStyle"}]}]}]}]},{"type":"paragraph","attrs":{"textAlign":"justify"},"content":[{"text":"Note:","type":"text","marks":[{"type":"textStyle"},{"type":"bold"}]},{"text":" Depending on the type of visa, specific documents will be required. For long-term visas, you will generally need to apply at the Vietnamese Embassy in India. ","type":"text","marks":[{"type":"textStyle"}]}]}]},"category":"General Information "},{"question":"What is Vietnam visa fees for Indians?","answer":{"type":"doc","content":[{"type":"paragraph","attrs":{"textAlign":"left"},"content":[{"text":"The Vietnam visa fee for Indian citizens is ","type":"text"},{"text":"₹2,150","type":"text","marks":[{"type":"bold"}]},{"text":" for a single entry (excluding any service fees). Atlys does not charge a service fee unless your e-visa is not issued within the expected timeframe. The visa is valid for 30 days and allows for a 30-day stay in Vietnam.","type":"text"}]}]},"category":"Application Process"},{"question":"Can I apply for a multiple-entry Vietnam visa?","answer":{"type":"doc","content":[{"type":"paragraph","attrs":{"textAlign":"left"},"content":[{"text":"Yes","type":"text","marks":[{"type":"bold"}]},{"text":", Indian citizens can apply for a ","type":"text"},{"text":"multiple-entry Vietnam visa","type":"text","marks":[{"type":"link","attrs":{"id":null,"rel":null,"url":"https://www.atlys.com/en-IN/visa/vietnam-90-day-multiple-entry-visa","slug":null,"type":null,"class":"link","locale":null,"target":null,"link_type":"web"}}]},{"text":" through the government website or Atlys. A multiple-entry Vietnam e-visa is valid for 90 days and allows you to stay in the country for up to 90 days. ","type":"text"}]}]},"category":"General Information"}]},"black_upsell":{"price":{"amount":330.53000000000003,"currency":"AED"},"eta":{"t0":1738356217965,"country":"VN","purpose":"atlys_black","visa_eta":1738745017952,"earliest_apply_on":1738356217952,"service_type":"visa","apply_delay":0,"display_eta":1738745017952},"entry_process":{"purpose":"atlys_black","service_fees":{"currency":"AED","discount_per_additional_purchase_amount":0,"distribution":[{"amount":293.8,"key":"partner_fees"},{"amo